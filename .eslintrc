{
  "extends": "airbnb-base",
  "rules": {
    "radix": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "max-len": ["error", { "code": 120 }],
    "import/prefer-default-export": "off",
    "no-loop-func": "off",
    "no-async-promise-executor": "off"
  }
}
